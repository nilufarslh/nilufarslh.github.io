source "https://rubygems.org"

gem "jekyll", "~> 4.3.2"
gem "bundler"
gem "jekyll-feed" # Generates an Atom feed for posts
gem "jekyll-seo-tag" # Adds SEO meta tags
gem "jekyll-sitemap" # Generates a sitemap.xml file
gem "jekyll-paginate" # Pagination for blog posts
gem "jekyll-theme-clean-blog" # Example theme (optional)
